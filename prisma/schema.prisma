// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum UsersType {
  MANUFRACTURER
  SUPPLIER
  DISTRIBUTOR
  RETAILER
  CUSTOMER
}
model Employee {
    id        Int      @id @default(autoincrement())
    nid       String   @db.VarChar(64) @unique
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
    name      String   @db.VarChar(255)
    phoneNo   String   @db.VarChar(64) @unique //@default(false)
    userType  UsersType
    // author    User     @relation(fields: [authorId], references: [id])
    // authorId  Int
}

model Users {
  id        Int      @id
  nid       String   @db.VarChar(64) @unique
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  name      String   @db.VarChar(255)
  licenseNo String   @db.VarChar(64) @unique
  phoneNo   String   @db.VarChar(64) //@default(false)
  userType  UsersType
  // author    User     @relation(fields: [authorId], references: [id])
  // authorId  Int
  
}
model Chains{
    id        Int      @id
    name      String   @db.VarChar(255)
    serialNo  String   @db.VarChar(255)
    hash      String   @db.VarChar(255)
    checkedBy String[] @db.VarChar(255)
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
    // author    User     @relation(fields: [authorId], references: [id])
    // authorId  Int
}


